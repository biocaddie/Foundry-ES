# a list of source descriptor configs
dryad: 
    sourceID: "biocaddie-0011"
    name: "Dryad"
    ingestMethod: "OAI"
    repositoryID: ""
    primaryKeyJSONPath: "$.record.header.identifier.'_$'"
    ingestURL: "http://www.datadryad.org/oai/request"
    sourceName: "Dryad"
    topElement: "ListRecords"
    documentElement: "record"
    useCache: "true"
    transformScript: "/Users/bozyurt/dev/java/Foundry-Data/transformations/dryad.trs"
clinicaltrials:
    sourceID: "biocaddie-0012"
    name: "Clinical Trials"
    ingestMethod: "WEB"
    primaryKeyJSONPath: "$.clinical_study.id_info.org_study_id.'_$'"
    ingestURL: "https://clinicaltrials.gov/search?term=&resultsxml=true"
    documentElement: "clinical_study"
    cacheFilename: "clinicaltrials_gov"
    filenamePattern: "\\w+\\.xml"
    useCache: "true"
    transformScript: "/Users/bozyurt/dev/java/Foundry-Data/transformations/clinicaltrials.trs"
lincs:
    sourceID: "biocaddie-0013"
    name: "NIH Library of Integrated Network-based Cellular Signatures Program"
    ingestMethod: "WEB"
    primaryKeyJSONPath: "$.datasetid"
    ingestURL: "http://dev3.ccs.miami.edu:8080/dcic/api/fetchdata?searchTerm=*&limit=500"
    documentElement: "documents"
    cacheFilename: "lincs_json"
    parserType: "json"
    useCache: "false"
    transformScript: "/home/ec2-user/dev/biocaddie/data-pipeline/transformations/lincs.trs"
arrayExpress: 
    sourceID: "biocaddie-0009"
    name: "ArrayExpress"
    ingestMethod: "XML"
    primaryKeyJSONPath: "$.experiment.id"
    ingestURL: "http://www.ebi.ac.uk/arrayexpress/xml/v2/experiments"
    topElement: "experiments"
    documentElement: "experiment"
    transformScript: "/Users/bozyurt/dev/java/Foundry-Data/transformations/arrayexpress.trs"
gemma:
    sourceID: "biocaddie-0007"
    name: "Gemma"
    ingestMethod: "CSV"
    primaryKeyJSONPath: "$.GemmaDsId"
    ingestURL: "http://www.chibi.ubc.ca/Gemma/datasetdownload/4.20.2011/DatasetSummary.view.txt.gz"
    transformScript: "/Users/bozyurt/dev/java/Foundry-Data/transformations/gemma.trs"
    keepMissing: "false"
    ignoreLines: "1"
    headerLine: "1"
    delimiter: "\t"
    textQuote: "&#034;"
pdb:
    sourceID: "biocaddie-0001"
    name: "RCSB Protein Data Bank"
    ingestMethod: "RSYNC"
    primaryKeyJSONPath: "$.'PDBx:datablock'.'@datablockName'"
    repositoryID: "nif-0000-00135"
    rysncSource: "rsync.wwpdb.org::ftp_data/structures/divided/XML-noatom/"
    rsyncDest: "/var/data/foundry-es/cache/data/pdb_rsync"
    fileNamePattern: "+\\.xml\\.gz$"
    largeRecords: "true"
    port: "33444"
    fullSet: "false"
    documentElement: "datablock"
    transformScript: "/Users/bozyurt/dev/java/Foundry-Data/transformations/pdb.trs"
bioproject:
    sourceID: "biocaddie-0008"
    name: "Bioproject"
    ingestMethod: "FTP"
    primaryKeyJSONPath: "$.'Package'.'Project'.'Project'.'ProjectID'.'ArchiveID'.'@accession'"
    transformScript: "/Users/bozyurt/dev/java/Foundry-Data/transformations/bioproject.trs"
    ftpHost: "ftp.ncbi.nlm.nih.gov"
    remotePath: "bioproject/bioproject.xml"
    topElement: "PackageSet"
    documentElement: "Package"
    outDir: "/var/data/foundry-es/cache/data"
...   
